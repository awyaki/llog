    "react": "^17.0.2",
    "react-ace": "^9.5.0",
    "react-datepicker": "^4.6.0",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.22.5",
    "react-icons": "^4.3.1",
    "react-router": "^5.2.1",
    "react-router-dom": "^5.3.0",
