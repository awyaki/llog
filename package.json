{
  "name": "llog",
  "version": "1.0.0",
  "description": "",
  "main": "./dist/main.js",
  "scripts": {
    "start": "npx webpack --config webpack.dev.js && npx electron ./dist/main.js",
    "watch": "npx webpack --config webpack.dev.js --watch",
    "build": "npx webpack --config webpack.prod.js"
  },
  "keywords": [],
  "author": "awyaki",
  "license": "ISC",
  "dependencies": {
    "@chakra-ui/react": "^1.7.1",
